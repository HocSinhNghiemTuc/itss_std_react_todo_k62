tss_std_react_todo_k62/src/reportWebVitals.js",[],"/home/hsnt/ドキュメント/Kỳ 8/ITSS 日本語/itss_std_react_todo_k62/src/components/Todo.js",["60"],"import React, {useState} from 'react';\n/* コンポーネント */\nimport TodoItem from './TodoItem';\nimport Input from './Input';\nimport Filter from './Filter';\n/* ライブラリ */\nimport {getKey} from \"../lib/util\";\nimport useStorageTodo from \"../hooks/storage_todo\";\nimport Auth from \"./Auth\";\n/* \n  【Todoのデータ構成】\n　・key：Todoを特定するID（String）\n　・text：Todoの内容（String）\n　・done：完了状態（Boolean true:完了済み,, false:未完了）\n*/\n\n/* カスタムフック */\n\nfunction Todo() {\n    const [items, addItem, updateItem, cleanItems] = useStorageTodo();\n\n    const handleCheckTodoItem = (item) => {\n        updateItem(item);\n    };\n    const handleAdd = (text) => {\n        const newItem = {key: getKey(), text: text, done: false};\n        addItem(newItem);\n    };\n    const [tab, setTab] = useState(\"すべて\");\n    const itemTab = () => {\n        return items.filter((item) => {\n            if (tab === \"すべて\") {\n                return item;\n            }\n            if (tab === \"未完了\" && !item.done) {\n                return item;\n            }\n            if (tab === \"完了済み\" && item.done) {\n                return item;\n            }\n        });\n    };\n    const handleChangeTab = (target) => {\n        setTab(target);\n    };\n    const handleCleanItem = () => {\n        cleanItems();\n    };\n    return (\n        <div className=\"panel\">\n            <Auth />\n            <div className=\"panel-heading\">\n                ITSS ToDoアプリ\n            </div>\n            <Input onAdd={handleAdd}/>\n            <Filter onClick={handleChangeTab}/>\n            {itemTab().map(item => (\n                <TodoItem item={item} key={item.key} onCheck={handleCheckTodoItem}/>\n            ))}\n            <div className=\"panel-block\">\n                {itemTab().length} items\n            </div>\n            <div className=\"panel-block\">\n                <button className=\"button is-light is-fullwidth\" onClick={handleCleanItem}>全てのToDoを削除</button>\n            </div>\n        </div>\n    );\n}\n\nexport default Todo;","/home/hsnt/ドキュメント/Kỳ 8/ITSS 日本語/itss_std_react_todo_k62/src/components/Input.js",[],"/home/hsnt/ドキュメント/Kỳ 8/ITSS 日本語/itss_std_react_todo_k62/src/components/Filter.js",["61"],"/* \n  【Filterコンポーネント】\n　・該当するTodoをステータス毎にで分けてリスト表示する\n　・タブで表示する\n　・サポートするステータスは「すべて」「未完了」「完了済み」\n*/\nfunction Filter({onClick}) {\n    const title = [\"すべて\", \"未完了\", \"完了済み\"];\n    const handleClick = (e) => {\n        e.preventDefault();\n        onClick(e.target.className);\n    };\n    return (\n        <div className=\"panel-tabs\">\n            {title.map((item, index) => (\n                <a\n                    href=\"#\"\n                    key={index}\n                    className={item}\n                    onClick={handleClick}\n                >{item}</a>\n            ))}\n        </div>\n    );\n}\n\nexport default Filter","/home/hsnt/ドキュメント/Kỳ 8/ITSS 日本語/itss_std_react_todo_k62/src/components/TodoItem.js",[],"/home/hsnt/ドキュメント/Kỳ 8/ITSS 日本語/itss_std_react_todo_k62/src/hooks/storage.js",[],"/home/hsnt/ドキュメント/Kỳ 8/ITSS 日本語/itss_std_react_todo_k62/src/lib/util.js",[],"/home/hsnt/ドキュメント/Kỳ 8/ITSS 日本語/itss_std_react_todo_k62/src/lib/firebase.js",[],"/home/hsnt/ドキュメント/Kỳ 8/ITSS 日本語/itss_std_react_todo_k62/src/components/Auth.js",["62"],"import React, {useEffect, useState} from \"react\";\nimport firebase,{uiConfig} from \"../lib/firebase\";\nimport {StyledFirebaseAuth} from \"react-firebaseui\";\nimport useStorageUser from \"../hooks/storage_user\";\n\n\nfunction Auth() {\n    const [users, addUser] = useStorageUser();\n    const [isSignedIn, setIsSignedIn] = useState(false);\n    useEffect(() => {\n        const unregisterAuthObserver = firebase.auth().onAuthStateChanged(user => {\n            if(user){\n                let flag = false;\n                for(let target of users){\n                    if(target.key === user.email){\n                        flag = true;\n                        break;\n                    }\n                }\n                if(!flag){\n                    addUser({key: user.email, name: user.displayName})\n                }\n            }\n            setIsSignedIn(!!user);\n        });\n        return () => unregisterAuthObserver();\n    }, []);\n    const signout = () => {\n        firebase.auth().signOut();\n    };\n    if (!isSignedIn) {\n        return (\n            <div className=\"column panel-block\">\n                <StyledFirebaseAuth uiConfig={uiConfig} firebaseAuth={firebase.auth()} />\n            </div>\n        );\n    }\n    return (\n        <div className=\"column panel-block\">\n            <p>{firebase.auth().currentUser.displayName}</p>\n            <button className=\"button is-danger is-light is-small\" onClick={signout}>Sign out</button>\n        </div>\n    );\n}\nexport default Auth;","/home/hsnt/ドキュメント/Kỳ 8/ITSS 日本語/itss_std_react_todo_k62/src/hooks/storage_user.js",[],"/home/hsnt/ドキュメント/Kỳ 8/ITSS 日本語/itss_std_react_todo_k62/src/hooks/storage_todo.js",[],{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","severity":1,"message":"68","line":31,"column":36,"nodeType":"69","messageId":"70","endLine":31,"endColumn":38},{"ruleId":"71","severity":1,"message":"72","line":15,"column":17,"nodeType":"73","endLine":20,"endColumn":18},{"ruleId":"74","severity":1,"message":"75","line":27,"column":8,"nodeType":"76","endLine":27,"endColumn":10,"suggestions":"77"},"no-native-reassign",["78"],"no-negated-in-lhs",["79"],"array-callback-return","Array.prototype.filter() expects a value to be returned at the end of arrow function.","ArrowFunctionExpression","expectedAtEnd","jsx-a11y/anchor-is-valid","The href attribute requires a valid value to be accessible. Provide a valid, navigable address as the href value. If you cannot provide a valid href, but still need the element to resemble a link, use a butt