Vitals.js",[],"/home/hsnt/ドキュメント/Kỳ 8/ITSS 日本語/itss_std_react_todo_k62/src/components/Todo.js",["60"],"import React, {useState} from 'react';\n/* コンポーネント */\nimport TodoItem from './TodoItem';\nimport Input from './Input';\nimport Filter from './Filter';\n/* ライブラリ */\nimport {getKey} from \"../lib/util\";\nimport useStorageTodo from \"../hooks/storage_todo\";\nimport Auth from \"./Auth\";\n/* \n  【Todoのデータ構成】\n　・key：Todoを特定するID（String）\n　・text：Todoの内容（String）\n　・done：完了状態（Boolean true:完了済み,, false:未完了）\n*/\n\n/* カスタムフック */\n\nfunction Todo() {\n    const [items, addItem, updateItem, cleanItems] = useStorageTodo();\n\n    const handleCheckTodoItem = (item) => {\n        updateItem(item);\n    };\n    const handleAdd = (text) => {\n        const newItem = {key: getKey(), text: text, done: false};\n        addItem(newItem);\n    };\n    const [tab, setTab] = useState(\"すべて\");\n    const itemTab = () => {\n        return items.filter((item) => {\n            if (tab === \"すべて\") {\n                return item;\n            }\n            if (tab === \"未完了\" && !item.done) {\n                return item;\n            }\n            if (tab === \"完了済み\" && item.done) {\n                return item;\n            }\n        });\n    };\n    const handleChangeTab = (target) => {\n        setTab(target);\n    };\n    const handleCleanItem = () => {\n        cleanItems();\n    };\n    return (\n        <div className=\"panel\">\n            <div className=\"panel-heading\">\n                ITSS ToDoアプリ\n            </div>\n            <Input onAdd={handleAdd}/>\n            <Filter onClick={handleChangeTab}/>\n            {itemTab().map(item => (\n                <TodoItem item={item} key={item.key} onCheck={handleCheckTodoItem}/>\n            ))}\n            <div className=\"panel-block\">\n                {itemTab().length} items\n            </div>\n            <div className=\"panel-block\">\n                <button className=\"button is-light is-fullwidth\" onClick={handleCleanItem}>全てのToDoを削除</button>\n            </div>\n        </div>\n    );\n}\n\nexport default Todo;","/home/hsnt/ドキュメント/Kỳ 8/ITSS 日本語/itss_std_react_todo_k62/src/components/Input.js",[],"/home/hsnt/ドキュメント/Kỳ 8/ITSS 日本語/itss_std_react_todo_k62/src/components/Filter.js",["61"],"/* \n  【Filterコンポーネント】\n　・該当するTodoをステータス毎にで分けてリスト表示する\n　・タブで表示する\n　・サポートするステータスは「すべて」「未完了」「完了済み」\n*/\nfunction Filter({onClick}) {\n    const title = [\"すべて\", \"未完了\", \"完了済み\"];\n    const handleClick = (e) => {\n        e.preventDefault();\n        onClick(e.target.className);\n    };\n    return (\n        <div className=\"panel-tabs\">\n            {title.map((item, index) => (\n                <a\n                    href=\"#\"\n                    key={index}\n                    className={item}\n                    onClick={handleClick}\n                >{item}</a>\n            ))}\n        </div>\n    );\n}\n\nexport default Filter","/home/hsnt/ドキュメント/Kỳ 8/ITSS 日本語/itss_std_react_todo_k62/src/components/TodoItem.js",[],"/home/hsnt/ドキュメント/Kỳ 8/ITSS 日本語/itss_std_react_todo_k62/src/hooks/storage.js",[],"/home/hsnt/ドキュメント/Kỳ 8/ITSS 日本語/itss_std_react_todo_k62/src/lib/util.js",[],"/home/hsnt/ドキュメント/Kỳ 8/ITSS 日本語/itss_std_react_todo_k62/src/lib/firebase.js",[],"/home/hsnt/ドキュメント/Kỳ 8/ITSS 日本語/itss_std_react_todo_k62/src/components/Auth.js",["62"],"/home/hsnt/ドキュメント/Kỳ 8/ITSS 日本語/itss_std_react_todo_k62/src/hooks/storage_user.js",[],"/home/hsnt/ドキュメント/Kỳ 8/ITSS 日本語/itss_std_react_todo_k62/src/hooks/storage_todo.js",[],{"ruleId":"63","replacedBy":"64"},{"ruleId":"65","replacedBy":"66"},{"ruleId":"67","severity":1,"message":"68","line":7,"column":8,"nodeType":"69","messageId":"70","endLine":7,"endColumn":12},{"ruleId":"71","severity":1,"message":"72","line":31,"column":36,"nodeType":"73","messageId":"74","endLine":31,"endColumn":38},{"ruleId":"75","severity":1,"message":"76","line":15,"column":17,"nodeType":"77","endLine":20,"endColumn":18},{"ruleId":"78","severity":1,"message":"79","lin